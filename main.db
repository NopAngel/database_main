-- Conectar a PostgreSQL y crear la base de datos
CREATE DATABASE compania_x;

-- Conectar a la base de datos
\c compania_x;

-- Crear la tabla usuarios
CREATE TABLE usuarios (
    id SERIAL PRIMARY KEY,
    nombre VARCHAR(50),
    apellido VARCHAR(50),
    email VARCHAR(100),
    fecha_registro DATE
);

